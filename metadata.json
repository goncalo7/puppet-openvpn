{
  "name": "dhoppe-openvpn",
  "version": "0.2.1",
  "author": "Dennis Hoppe",
  "summary": "This module installs, configures and manages the OpenVPN service.",
  "license": "Apache-2.0",
  "source": "git://github.com/dhoppe/puppet-openvpn.git",
  "project_page": "https://github.com/dhoppe/puppet-openvpn",
  "issues_url": "https://github.com/dhoppe/puppet-openvpn/issues",
  "dependencies": [
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 4.5.0 < 5.0.0"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "6",
        "7",
        "8"
      ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "12.04",
        "14.04"
      ]
    }
  ],
  "requirements": [
    {
      "name": "pe",
      "version_requirement": ">= 3.2.0"
    },
    {
      "name": "puppet",
      "version_requirement": ">= 3.2.0"
    }
  ],
  "tags": [
    "openvpn"
  ]
}
